@model IEnumerable<WeeklyGamingBASIC.Models.Members>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.strFirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.strLastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.strSteamAccount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.strBlizzardAccount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.strEAAccount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.strThreeDSFriendCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.strMinecraftAccount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.strLeagueOfLegendsAccount)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.strFirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.strLastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.strSteamAccount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.strBlizzardAccount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.strEAAccount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.strThreeDSFriendCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.strMinecraftAccount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.strLeagueOfLegendsAccount)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.intMemberID }) |
            @Html.ActionLink("Details", "Details", new { id=item.intMemberID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.intMemberID })
        </td>
    </tr>
}

</table>
